#include <iostream>
using namespace std;

int main()
{
	int t,n, k, gd, fd,sum=0;
	cin >> t;
	if (t>=1 && t<= 1000)
	{
		for (int x= 0; x<t; x++)
		{
			cin >> n >> k >> gd;
			int *arr = new int[n];
			if (n >= 1 && n <= 100 && k >= 1 && k <= pow(10, 9) && gd >= 1 && gd <= pow(10, 9))
			{
				for (int i = 0; i < n; i++)
				{
					cin >> arr[i];
					if (!(arr[i] >= 1 && arr[i] <= pow(10, 7)))
						return -1;
					else
						sum = sum + arr[i];
				}
				//cout << sum << endl;
				fd = sum / k;
				//cout << fd;
				if (fd > gd)
					cout << gd;
				else
					cout << fd;
			}
			else
				return -1;
		}
	}
	else
		return -1;
	return 0;
}

